<view><searchbar bind:searchInput="__e" vue-id="1" data-event-opts="{{[['^searchInput',[['onSearchInputEvent']]]]}}" bind:__l="__l"></searchbar><block wx:if="{{historys.length>0&&!subjects.length>0}}"><view class="history-list-group"><view class="history-title"><view class="title">历史记录</view><view data-event-opts="{{[['tap',[['onClearEvent',['$event']]]]]}}" class="clear" bindtap="__e">清除</view></view><block wx:for="{{historys}}" wx:for-item="item" wx:for-index="index" wx:key="index"><navigator class="history-group">{{item.title}}</navigator></block></view></block><view class="item-list-group"><block wx:for="{{subjects}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['onItemTapEvent',['$0'],[[['subjects','',index]]]]]]]}}" class="item-group" bindtap="__e"><image class="thumbnail" src="{{item.image}}"></image><view class="info-group"><view class="title">{{item.title}}</view><view class="rate-year">{{item.rating.average+"分/"+item.author}}</view></view></view></block></view></view>