<view class="comment-group"><view class="left-comment"><image class="avatar" src="{{item.author.avatar}}"></image></view><view class="right-comment"><view class="username-rate"><text class="username">{{item.author.name}}</text><star vue-id="1" rate="{{item.rating.value}}" starsize="{{30}}" istext="{{false}}" bind:__l="__l"></star></view><view class="release-time">{{item.created_at}}</view><view class="content">{{item.content}}</view></view></view>